 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 2 "q";
L_0x562b625f9200 .functor NOT 1, v0x562b625f8380_0, C4<0>, C4<0>, C4<0>;
L_0x562b625f92c0 .functor NOT 1, v0x562b625f8380_0, C4<0>, C4<0>, C4<0>;
L_0x562b625f9330 .functor NOT 1, v0x562b625f8990_0, C4<0>, C4<0>, C4<0>;
