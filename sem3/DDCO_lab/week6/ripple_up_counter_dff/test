S_0x562b625e0980 .scope module, "tb_ripple_up_counter_2bit" "tb_ripple_up_counter_2bit" 2 3;
 .timescale -9 -12;
v0x562b625f8fb0_0 .var "clk", 0 0;
v0x562b625f90a0_0 .net "q", 1 0, L_0x562b625f9420;  1 drivers
v0x562b625f9160_0 .var "rst", 0 0;
