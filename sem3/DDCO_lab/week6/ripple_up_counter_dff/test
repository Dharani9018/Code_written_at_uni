v0x562b625f8cb0_0 .net "q0", 0 0, v0x562b625f8380_0;  1 drivers
v0x562b625f8d80_0 .net "q1", 0 0, v0x562b625f8990_0;  1 drivers
v0x562b625f8e50_0 .net "rst", 0 0, v0x562b625f9160_0;  1 drivers
L_0x562b625f9420 .concat [ 1 1 0 0], v0x562b625f8380_0, v0x562b625f8990_0;
S_0x562b625e2850 .scope module, "ff0" "dff" 3 22, 3 1 0, S_0x562b625e0b10;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 1 "d";
    .port_info 3 /OUTPUT 1 "q";
v0x562b625e3370_0 .net "clk", 0 0, v0x562b625f8fb0_0;  alias, 1 drivers
v0x562b625f82c0_0 .net "d", 0 0, L_0x562b625f9200;  1 drivers
v0x562b625f8380_0 .var "q", 0 0;
v0x562b625f8420_0 .net "rst", 0 0, v0x562b625f9160_0;  alias, 1 drivers
E_0x562b625a7ba0 .event posedge, v0x562b625f8420_0, v0x562b625e3370_0;
S_0x562b625f8560 .scope module, "ff1" "dff" 3 25, 3 1 0, S_0x562b625e0b10;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 1 "d";
    .port_info 3 /OUTPUT 1 "q";
v0x562b625f87f0_0 .net "clk", 0 0, L_0x562b625f92c0;  1 drivers
v0x562b625f88d0_0 .net "d", 0 0, L_0x562b625f9330;  1 drivers
v0x562b625f8990_0 .var "q", 0 0;
v0x562b625f8a30_0 .net "rst", 0 0, v0x562b625f9160_0;  alias, 1 drivers
E_0x562b625a7e70 .event posedge, v0x562b625f8420_0, v0x562b625f87f0_0;
    .scope S_0x562b625e2850;
T_0 ;
    %wait E_0x562b625a7ba0;
    %load/vec4 v0x562b625f8420_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x562b625f8380_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x562b625f82c0_0;
    %assign/vec4 v0x562b625f8380_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x562b625f8560;
T_1 ;
    %wait E_0x562b625a7e70;
    %load/vec4 v0x562b625f8a30_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x562b625f8990_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x562b625f88d0_0;
    %assign/vec4 v0x562b625f8990_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x562b625e0980;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x562b625f8fb0_0, 0, 1;
T_2.0 ;
    %delay 5000, 0;
    %load/vec4 v0x562b625f8fb0_0;
    %inv;
    %store/vec4 v0x562b625f8fb0_0, 0, 1;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_0x562b625e0980;
T_3 ;
    %vpi_call 2 24 "$dumpfile", "ripple_up_counter_2bit.vcd" {0 0 0};
    %vpi_call 2 25 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x562b625e0980 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x562b625f9160_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x562b625f9160_0, 0, 1;
    %delay 100000, 0;
    %vpi_call 2 36 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x562b625e0980;
T_4 ;
    %vpi_call 2 41 "$monitor", "Time=%0t | clk=%b rst=%b | q=%b", $time, v0x562b625f8fb0_0, v0x562b625f9160_0, v0x562b625f90a0_0 {0 0 0};
    %end;
    .thread T_4;
