E_012790F0 .event posedge, v0127AE58_0, v01274DE0_0;
    .scope S_01277CB0;
T_0 ;
    %wait E_012790F0;
    %load/v 8, v0127AE58_0, 1;
    %jmp/0xz  T_0.0, 8;
    %ix/load 0, 2, 0;
    %assign/v0 v0127ADA8_0, 0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v01271058_0, 2;
    %ix/load 0, 2, 0;
    %assign/v0 v0127ADA8_0, 0, 8;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_01277CB0;
T_1 ;
    %wait E_01278F30;
    %set/v v01271000_0, 0, 1;
    %load/v 8, v0127ADA8_0, 2;
    %cmpi/u 8, 0, 2;
    %jmp/1 T_1.0, 6;
    %cmpi/u 8, 1, 2;
    %jmp/1 T_1.1, 6;
    %cmpi/u 8, 2, 2;
    %jmp/1 T_1.2, 6;
    %cmpi/u 8, 3, 2;
    %jmp/1 T_1.3, 6;
    %set/v v01271058_0, 0, 2;
    %jmp T_1.5;
T_1.0 ;
    %load/v 8, v01270FA8_0, 2;
    %cmpi/u 8, 1, 2;
    %jmp/0xz  T_1.6, 4;
    %movi 8, 1, 2;
    %set/v v01271058_0, 8, 2;
    %jmp T_1.7;
T_1.6 ;
    %load/v 8, v01270FA8_0, 2;
    %cmpi/u 8, 2, 2;
    %jmp/0xz  T_1.8, 4;
    %movi 8, 2, 2;
    %set/v v01271058_0, 8, 2;
    %jmp T_1.9;
T_1.8 ;
    %set/v v01271058_0, 0, 2;
T_1.9 ;
T_1.7 ;
    %jmp T_1.5;
T_1.1 ;
    %load/v 8, v01270FA8_0, 2;
    %cmpi/u 8, 1, 2;
    %jmp/0xz  T_1.10, 4;
    %movi 8, 2, 2;
    %set/v v01271058_0, 8, 2;
    %jmp T_1.11;
T_1.10 ;
    %load/v 8, v01270FA8_0, 2;
    %cmpi/u 8, 2, 2;
    %jmp/0xz  T_1.12, 4;
    %set/v v01271058_0, 1, 2;
    %set/v v01271000_0, 1, 1;
    %jmp T_1.13;
T_1.12 ;
    %movi 8, 1, 2;
    %set/v v01271058_0, 8, 2;
T_1.13 ;
T_1.11 ;
    %jmp T_1.5;
T_1.2 ;
    %load/v 8, v01270FA8_0, 2;
    %cmpi/u 8, 1, 2;
    %jmp/0xz  T_1.14, 4;
    %set/v v01271058_0, 1, 2;
    %set/v v01271000_0, 1, 1;
    %jmp T_1.15;
T_1.14 ;
    %load/v 8, v01270FA8_0, 2;
    %cmpi/u 8, 2, 2;
    %jmp/0xz  T_1.16, 4;
    %set/v v01271058_0, 1, 2;
    %set/v v01271000_0, 1, 1;
    %jmp T_1.17;
T_1.16 ;
    %movi 8, 2, 2;
    %set/v v01271058_0, 8, 2;
T_1.17 ;
T_1.15 ;
    %jmp T_1.5;
T_1.3 ;
    %set/v v01271058_0, 0, 2;
    %set/v v01271000_0, 0, 1;
    %jmp T_1.5;
T_1.5 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_01277A90;
T_2 ;
    %set/v v0127AEB0_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_01277A90;
T_3 ;
    %delay 5000, 0;
    %load/v 8, v0127AEB0_0, 1;
    %inv 8, 1;
    %set/v v0127AEB0_0, 8, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_01277A90;
T_4 ;
    %set/v v0127A078_0, 1, 1;
    %delay 12000, 0;
    %set/v v0127A078_0, 0, 1;
    %end;
    .thread T_4;
    .scope S_01277A90;
T_5 ;
    %vpi_call 2 36 "$dumpfile", "vending_machine_tb.vcd";
